<config>
	<acl>
		<resources>
			<all>
				<title>Allow Everything</title>
			</all>
			<admin>
				<children>
					<system>
						<children>
							<config>
								<children>
									<imagezoomconfig translate="title">
										<title>ImageZoom Config Section</title>
										<sort_order>0</sort_order>
									</imagezoomconfig>
								</children>
							</config>
						</children>
					</system>
				</children>
			</admin>
		</resources>
	</acl>
</config>